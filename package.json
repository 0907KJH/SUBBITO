  "author": "SUBBITO Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0907KJH/subbito-app.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
  "release": "npm run build && powershell -ExecutionPolicy ByPass -File tools\\publish_copy.ps1",
    "compare:live": "powershell -ExecutionPolicy ByPass -File tools\\fetch_and_compare.ps1",
    "fetch:assets": "powershell -ExecutionPolicy ByPass -File tools\\fetch_all_assets.ps1",
    "compare:full": "npm run compare:live && npm run fetch:assets",
    "apply:live": "powershell -ExecutionPolicy ByPass -File tools\\copy_live_assets_to_public.ps1",
    "serve:live": "node tools/serve_live.js"
  },
  "scriptsPrecedenceNote": "serve:live uses node tools/serve_live.js to serve public/index.live.html on port 5178",
  "dependencies": {
    "@tanstack/react-query": "^4.35.0",
    "date-fns": "^2.30.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.278.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0"
  }
}
